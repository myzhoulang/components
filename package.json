{
  "name": "@zhou.lang/components",
  "version": "1.0.17",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish --access=public",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "files": [
    "dist",
    "es",
    "lib"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "antd-img-crop": "^3.14.1",
    "braft-editor": "^2.3.9",
    "spark-md5": "^3.0.1",
    "xss": "^1.0.9"
  },
  "devDependencies": {
    "@types/draft-js": "^0.11.3",
    "@types/spark-md5": "^3.0.2",
    "@umijs/test": "^3.0.5",
    "antd": "^4.16.1",
    "babel-plugin-import": "^1.13.3",
    "dumi": "^1.0.14",
    "father-build": "^1.17.2",
    "gh-pages": "^3.1.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "yorkie": "^2.0.0"
  },
  "resolutions": {
    "@types/react": "^16.14.2"
  },
  "peerDependencies": {
    "@ant-design/icons": "^4.6.2",
    "antd": "^4.16.1",
    "classnames": "^2.3.1",
    "react": "^17.0.2"
  },
  "sideEffects": [
    "./src/**/styles/**"
 ]
}
